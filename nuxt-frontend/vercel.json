{
  "buildCommand": "npm run build",
  "outputDirectory": ".output/public",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nuxtjs",
  "regions": ["hkg1"],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NUXT_PUBLIC_API_BASE": "$NUXT_PUBLIC_API_BASE",
      "NUXT_PUBLIC_SITE_URL": "$NUXT_PUBLIC_SITE_URL"
    }
  }
}